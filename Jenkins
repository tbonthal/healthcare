pipeline
 {
  agent any
  stages{
   
   stage('Build Application'){
   steps{
   bat 'mvn clean install'
   }
   }
   
   
   stage('SonarQube Quality Check'){
   steps{
   bat 'mvn sonar:sonar -Dsonar.sources=src/ -Dsonar.host.url=http://localhost:9000 -Dsonar.login=0d5d7a79f852103dd4711a06add3fd6dccd6ded3'
   }
   }
   stage('Deploy Application in Mulesoft Cloudhub'){
   steps{
   bat 'mvn package deploy -DmuleDeploy'
   }  
   }
  }
}